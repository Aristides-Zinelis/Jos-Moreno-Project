{% extends 'ArisFrontBundle::layout.html.twig' %}
{% block title %}Jose {% endblock %}
{% block styles %} 
    <link href="{{ asset('css/style.css')}}" rel="stylesheet" />
    <link href="{{ asset('css/font-awesome.min.css')}}" rel="stylesheet" />
    
    <link href="{{ asset('css/custom.css')}}" rel="stylesheet" />
    <link href="{{ asset('css/animate.css')}}" rel="stylesheet" />
{% endblock %}
{% block scripts %}
  
{% endblock %}
{% block content %} 
    <div> 
              <img style="position: fixed; width: 100%; height:100%; " src="{{asset('images/design/slide-wrapper.png')}}"  />  
         
               <video  class="video-js vjs-default-skin" style=" width: 100%; height:100%;" poster="{{ asset('videos/poster.jpg')}}" muted autoplay preload loop>
                    <source src="{{ asset('videos/CameraLens.mp4')}}" type='video/mp4'/>
                    <source src="{{ asset('videos/CameraLens.webm')}}" type='video/webm' />
                    <source src="{{ asset('videos/CameraLens.ogv')}}" type='video/ogg' />
                </video>
          
              
               	<h2 id="anim1" style="top:20%; position: absolute">Con los ojos de</h2>
                <h1 id="anim2" style="top:25%; position: absolute">Jos√© Moreno</h1>
              	<p id="anim3" style="top:35%; position: absolute">Momentos capturados por la camara!</p>
           
            </div>
      </div>
{% endblock %} 
{% block finalScripts %}
     <!-- Easing core JavaScript -->
    <script language="javascript" src="{{ asset('js/jquery.easing.1.3.js')}}"></script>
	<!-- Bootstrap core JavaScript -->
    <script language="javascript" src="{{ asset('js/bootstrap.js')}}"></script>
    <!-- Master slider core JavaScript-->
    <script language="javascript" src="{{ asset('js/video.js')}}"></script>
   
    <script language="javascript" src="{{ asset('js/touchTouch.jquery.js')}}" ></script>
     <script language="javascript" src="{{ asset('js/jquery.animation.js')}}"></script>
     <script language="javascript">
         
         
    $('#anim1').addClass('animated fadeInLeft');
    $('#anim2').addClass('animated fadeInDown');
    $('#anim3').addClass('animated fadeInRight');

     </script>
{% endblock %} 