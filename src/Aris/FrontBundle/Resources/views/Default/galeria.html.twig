{% extends 'ArisFrontBundle::layout.html.twig' %}
{% block title %}Jose {% endblock %}
{% block styles %} 
    <link href="{{ asset('css/style.css')}}" rel="stylesheet" />
    <link href="{{ asset('css/font-awesome.min.css')}}" rel="stylesheet" />
    <link href="{{ asset('css/settings.css')}}" rel="stylesheet" />
    <link rel="stylesheet" href="{{ asset('css/normalize.css')}}" type="text/css" media="screen">
    <link href="{{ asset('css/custom.css')}}" rel="stylesheet" />
    <link href="{{ asset('css/lightbox.css')}}" rel="stylesheet" />
    <link href="{{ asset('css/isotope.css')}}" rel="stylesheet" />
    <link rel="stylesheet" href="{{ asset('css/touchTouch.css')}}" type="text/css" media="screen">
    <link rel="stylesheet" href="{{ asset('css/galerias.css')}}" type="text/css" media="screen">
    
   
{% endblock %}
{% block scripts %}
  
{% endblock %}
{% block content %} 
   
    
   
   	
    	
        <div class="container" style="z-index: 5; padding-top: 14%;">
            <div class="row">
               <!--Start heading-->
               <div class="col-md-offset-2 col-md-8">
                    <div class="section-header">
                        <h2 class="smt"><span>{{galeria.name}}</span></h2>
                    </div>
                </div>
                <!--End heading-->
                 <div class="clear"></div>
                <ul id="selector" class="portfolio-filters text-center" >
                     <li data-filter="all" class="filter active">todas</li>
                     {% for sec in secuencias %} 
                  
                    <li data-filter="{{sec.secuencia}}" class="filter">{{sec.secuencia}}</li>
                   
                    {% endfor%}  
                </ul><!--portfolio-filter nav-->
            
            <div id="grid">
                
               
                     
               {% for img in galeria.images %} 
                   
              <div class="col-md-4 col-sm-6 mix all {{img.secuencia}} mix_all" style="display: inline-block;  opacity: 1;">
                     <a title="{{img.nombre}}" data-gallery="{{img.secuencia}}" href="{{asset('uploads/images/')}}{{img.image}}" class="magnifier">
                       <div class="image-wrapper">
                            <img alt="work" class="img-responsive thumbnail" src="{{asset('uploads/images/')}}{{img.image}}">
                       <div class="image-overlay">
                           <span><i class="fa fa-eye"></i></span>
                        </div>
                      </div>
                     </a>
                </div> <!--work item-->
                
              
               {% endfor%}  
              
              </div> <!--/ grid-->
           </div>  <!-- /row-->
        </div> <!-- /container-->

             
  
{% endblock %} 
{% block finalScripts %}
     <!-- Easing core JavaScript -->
    <script language="javascript" src="{{ asset('js/jquery.easing.1.3.js')}}"></script>
	<!-- Bootstrap core JavaScript -->
    <script language="javascript" src="{{ asset('js/bootstrap.js')}}"></script>
    
    <script language="javascript" src="{{ asset('js/touchTouch.jquery.js')}}" ></script>
    <script language="javascript" src="{{ asset('js/waypoints.min.js')}}"></script>
    <script type="text/javascript" src="{{ asset('js/jquery.mixitup.min.js')}}"></script>
            
              <script type="text/javascript">
                  
                   jQuery('#grid').mixitup({
		targetSelector: '.mix',
		});
                
               	jQuery('.magnifier').touchTouch();
		
  		  	
	
              </script>
{% endblock %} 

